
@model WebApplication_Moby_Dick.Models.liste
@{
 Layout = null;
 }

<!DOCTYPE html>

<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Index</title>
    </head>
    <body>
        <table cellpadding="0" cellspacing="0">
            <tr>
                <th>Text</th>
                <th>Count</th>

            </tr>
            @foreach (var item in Model.kelimeler)
            {
            <tr>
                <td>@Html.DisplayTextFor(model => item.text)</td>
                <td>@Html.DisplayTextFor(model => item.count)</td>
            </tr>
            }
        </table>
    </body>
</html>

